{
  "name": "ngnode-fullstack",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Asad Sahi",
  "homepage": "http://ngnode-fullstack.herokuapp.com",
  "description": "Node and Angular fullstack SPA",
  "engines": {
    "node": "8.9.4"
  },
  "angular-cli": {},
  "scripts": {
    "clean": "rimraf dist dist-server server.js prerender.js",
    "db": "sequelize db:migrate & sequelize db:seed:all",
    "ssr:serve": "webpack && node server.js",
    "build:prod": "npm run clean && ng build --prod && ng build --prod --app 1 && webpack --progress --colors",
    "serve:prod": "npm run build:prod && node server.js",
    "server": "node server.js",
    "serve:dev:client": "concurrently \"ng serve --extract-css --proxy-config proxy.conf.js --open\" nodemon",
    "serve:dev:ssr": "concurrently \"ng build --dev --aot=true --output-hashing=all --extract-css --named-chunks=false --watch\" \"ng build --dev --aot=true --output-hashing=all --named-chunks=false --app 1 --watch\" nodemon",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate:prerender": "node prerender.js",
    "postinstall": "npm run build:prod",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "bundle:analyze": "ng build --extract-css --prod --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
    "deploy:heroku": "heroku config:set NPM_CONFIG_PRODUCTION=false && git push heroku master && heroku open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.2",
    "@angular/common": "5.2.2",
    "@angular/compiler": "5.2.2",
    "@angular/core": "5.2.2",
    "@angular/forms": "5.2.2",
    "@angular/http": "5.2.2",
    "@angular/platform-browser": "5.2.2",
    "@angular/platform-browser-dynamic": "5.2.2",
    "@angular/platform-server": "5.2.2",
    "@angular/router": "5.2.2",
    "@angular/service-worker": "5.2.2",
    "@nguniversal/express-engine": "5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "5.0.0-beta.5",
    "acl": "0.4.11",
    "angular2-jwt": "0.2.3",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.2",
    "bootstrap": "4.0.0",
    "compression": "1.7.1",
    "cookie-parser": "1.4.3",
    "core-js": "2.5.3",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "globby": "7.1.1",
    "hbs": "4.0.1",
    "helmet": "3.10.0",
    "jsonwebtoken": "8.1.1",
    "lodash": "4.17.4",
